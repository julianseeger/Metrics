{
    "name": "julianseeger/metrics-server",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
      "silex/silex":  "*",
      "twig/twig":    ">=1.8",
      "sabre/xml" : "master-dev"
    },
    "require-dev": {
      "phpunit/phpunit":                  "3.7.*",
      "phpmd/phpmd" :                     "1.5.0",
      "squizlabs/php_codesniffer":        "1.*",
      "theseer/phpdox":                   "0.6.5",
      "mikey179/vfsStream":               ">=1.2.0",
      "pdepend/pdepend" :                 "1.1.1",
      "sebastian/phpcpd":                 "*"
    },
    "autoload": {
      "psr-4": {
        "Metrics\\Web\\": ["web/php/src/", "web/php/tests"],
        "Metrics\\Core\\": ["core/src", "core/tests"]
      }
    }
}
